// Previous voltage for lightning effect
float prevVoltage = 0.0;

// --- Blynk Timer ---
BlynkTimer timer;

// --- Function to send data to Blynk ---
void sendToBlynk(float voltage, long steps) {
  if (Blynk.connected()) {   // Only send if connected
    Blynk.virtualWrite(V0, voltage);
    Blynk.virtualWrite(V1, voltage);
    Blynk.virtualWrite(V2, steps);
  }
}

// --- Fade-in Logo Function ---
void showLogoFadeIn() {
  for (int i = 0; i < 5; i++) {       // 5 fade steps
    display.clearDisplay();
    display.drawBitmap(0, 0, epd_bitmap_Your_paragraph_text__3_, 128, 64, SSD1306_WHITE);
    display.display();
    delay(300); // pause between steps
  }
  delay(2000); // hold the final logo a bit longer
  display.clearDisplay();
  display.display();
}

void setup()
{
  Serial.begin(115200);

  // --- Non-blocking WiFi + Blynk ---
  WiFi.begin(ssid, pass);
  Blynk.config(BLYNK_AUTH_TOKEN);

  // Relay setup
  pinMode(RELAY_PIN, OUTPUT);
  digitalWrite(RELAY_PIN, LOW);

  // OLED setup
  if (!display.begin(SSD1306_SWITCHCAPVCC, OLED_ADDR)) {
    Serial.println(F("SSD1306 allocation failed"));
    while (1);
  }

  // --- Show animated logo ---
  showLogoFadeIn();

  // INA226 setup
  Wire.begin();
  if (!INA.begin()) {
    Serial.println("INA226 Error!");
    display.clearDisplay();
    display.setCursor(0,0);
    display.println("INA226 Error!");
    display.display();
    while (1);
  }
  INA.setMaxCurrentShunt(1, 0.002);

  // Blynk timer: send every 1s
  timer.setInterval(1000L, [](){
    float busVoltage = INA.getBusVoltage();

    // Calculate steps left
    long remainingSteps = 0;
    if (busVoltage < 5.5) {
      remainingSteps = (long)((5.5 - busVoltage) / STEP_SIZE);
    }

    sendToBlynk(busVoltage, remainingSteps);
  });
}

void loop()
{
  float busVoltage = INA.getBusVoltage();

  // Relay control logic
  if (!relayState && busVoltage >= VOLTAGE_ON) {   
    digitalWrite(RELAY_PIN, HIGH);  
    relayState = true; 
    Serial.println("Relay ON: Capacitor connected");
  }
  else if (relayState && busVoltage <= VOLTAGE_OFF) {
    digitalWrite(RELAY_PIN, LOW);   
    relayState = false;
    Serial.println("Relay OFF: Capacitor disconnected");
  }

  // Remaining steps
  long remainingSteps = 0;
  if (busVoltage < 5.5) {
    remainingSteps = (long)((5.5 - busVoltage) / STEP_SIZE);
  }

  // Debug
  Serial.print("Stored Voltage: "); Serial.print(busVoltage, 3); Serial.println(" V");
  Serial.print("Remaining Steps: "); Serial.println(remainingSteps);
  Serial.print("Relay: "); Serial.println(relayState ? "ON" : "OFF");
  Serial.println("----------------------------");

  // --- OLED Update ---
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(SSD1306_WHITE);
  display.setCursor(10,15);
  display.println("PIEZO");

  display.setCursor(85,15);
  display.println("BATTERY");

  display.setTextSize(2);
  display.setCursor(0,25);
  display.print(busVoltage, 2);
  display.println("V");

  display.setTextSize(2);
  display.setCursor(79,25);
  display.print("100");
  display.println("%");

  display.setTextSize(1);
  display.setCursor(15,44);
  display.print("Steps left: ");
  display.println(remainingSteps);

  int barWidth = map(busVoltage * 100, 0, 500, 0, SCREEN_WIDTH - 35); 
  display.drawRect(0, 54, SCREEN_WIDTH - 35, 8, SSD1306_WHITE); 
  display.fillRect(0, 54, barWidth, 8, SSD1306_WHITE);

  display.setTextSize(1);
  display.setCursor(SCREEN_WIDTH - 31, 54);
  display.print("5.5V");

  display.drawBitmap(0, 0, epd_bitmap_battery, 128, 64, SSD1306_WHITE);

  // --- WiFi Icon ---
  if (WiFi.status() == WL_CONNECTED) {
    display.drawBitmap(0, 0, epd_bitmap_wifi, 128, 64, SSD1306_WHITE);
    Blynk.run();
  } else {
    display.drawBitmap(0, 0, epd_bitmap_no_wifi, 128, 64, SSD1306_WHITE);
  }

  if (busVoltage >= 5.5) {
    display.clearDisplay();

    display.setTextSize(1);
    display.setTextColor(SSD1306_WHITE);
    display.setCursor(13,10);
    display.println("Reached 5.5 Volts");

    display.drawBitmap(0, 0, epd_bitmap_full_v, 128, 64, SSD1306_WHITE);

    display.setTextSize(1);
    display.setTextColor(SSD1306_WHITE);
    display.setCursor(0,55);
    display.println("Storing Power to Batt");

    delay(2000);
  } 

  display.display();

  timer.run();

  delay(500);
}

